    "i18next-http-backend": "^1.3.1",
    "nanoid": "^3.1.30",
    "normalize.css": "^8.0.1",
    "proptypes": "^1.1.0",
    "react": "^17.0.2",
    "react-adal": "^0.5.2",
    "react-dom": "^17.0.2",
