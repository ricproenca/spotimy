{
  "name": "spotimy",
  "version": "0.0.1",
  "description": "Spotify Playlists Management App",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricproenca/spotimy.git"
  },
  "keywords": [
    "spotify",
    "playlists",
    "management"
  ],
  "author": "Ricardo Proenca",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ricproenca/spotimy/issues"
  },
  "homepage": "https://github.com/ricproenca/spotimy#readme",
  "scripts": {
    "start": "webpack-dev-server -- --config ./webpack/webpack.config.js --env mode=development",
    "dev": "cross-env NODE_ENV=development webpack -- --config ./webpack/webpack.config.js --env mode=development",
    "prod": "cross-env NODE_ENV=production webpack -- --config ./webpack/webpack.config.js --env mode=production",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\" --ignore-path .gitignore",
    "lint": "eslint tests/**/*.js config/**/*.js src/**/*.{js,jsx} --ignore-path .gitignore",
    "lint:fix": "eslint test/**/*.js config/**/*.js src/**/*.{js,jsx} --fix --ignore-path .gitignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "test:update": "jest --updateSnapshot",
    "report:bundle": "http-server ./reports/bundle",
    "report:coverage": "http-server ./reports/tests/lcov-report/",
    "generate": "plop",
    "demo:dev": "npm run dev && http-server ./dist",
    "demo:prod": "npm run prod && http-server ./dist"
  },
